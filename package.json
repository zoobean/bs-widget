{
  "name": "bs-widget",
  "version": "1.0.0",
  "description": "",
  "main": "dist/bundle.js",
  "type": "module",
  "scripts": {
    "start": "rollup --config -w",
    "serve": "http-server . -p 8080 -c-1",
    "serve:open": "http-server . -p 8080 -c-1 -o /index.html",
    "dev": "concurrently \"yarn start\" \"yarn serve\"",
    "dev:open": "concurrently \"yarn start\" \"yarn serve:open\"",
    "build": "rollup --config",
    "clean": "rm -f dist/bundle.js",
    "rebuild": "yarn clean && yarn build",
    "test": "yarn build && node --test",
    "test:smoke": "yarn build && node --test test/smoke.test.js",
    "verify": "yarn test",
    "prepublishOnly": "yarn verify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "dependencies": {
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "concurrently": "^9.2.1",
    "http-server": "^14.1.1",
    "rollup": "^3.29.5"
  }
}
